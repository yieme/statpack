  "bugs": {
    "url": "https://github.com/yieme/statpack/issues"
  },
  "app": {
    "port": 3000
  },
  "devDependencies": {
    "jshint": "^2.6.0",
    "mocha": "^2.1.0",
    "pkg-cli": "^0.3.0",
    "should": "^4.6.5"
  },
  "dependencies": {
    "install": "^0.1.8",
    "npm": "^2.5.0",
    "simple-statistics": "^0.9.0"
  }
}